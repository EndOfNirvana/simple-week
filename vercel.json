{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "dist/client",
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "api/server.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 10
    }
  }
}
